/*
 * 1-150循环车输出
 * 在3的倍数杭商打印出Foo
 * 在5的倍数杭商打印出Biz
 * 在3的倍数杭商打印出Bia
 * */
package com.lzw;

public class FooBizBaz {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int i = 1; i <= 150; i++) {
			System.out.print(i);
			if(i % 3 == 0) {
				System.out.println("Foo");				
			}else if(i % 5 == 0) {
				System.out.println("Biz");				
			}else if(i % 7 == 0) {
				System.out.println("Bia");				
			}else {
				System.out.println();				
			}
		}
	}

}
